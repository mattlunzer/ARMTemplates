{
//Required section that describes the structure of JSON data. The version number you use depends on the scope of the deployment.
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",

//Required section that defines the version of your template (such as 1.0.0.0). 
  "contentVersion": "",

//Optional section that defines a collection of API versions for resource types. You can use this value to avoid having to specify API versions for each resource in the template.
  "apiProfile": "",

//"Optional" section where you define values that are provided during deployment. These values can be provided by a parameter file, by command-line parameters, or in the Azure portal.
  "parameters": {  },

//An optional section where you define values that are used to simplify template language expressions.
  "variables": {  },

//An optional section where you can define user-defined functions that are available within the template. User-defined functions can simplify your template when complicated expressions are used repeatedly in your template.
  "functions": [  ],

//A required section that defines the actual items you want to deploy or update in a resource group or a subscription.
  "resources": [  ],

//An optional section where you specify the values that will be returned at the end of the deployment.
  "outputs": {  }
}


